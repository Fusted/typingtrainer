{
    "name": "typingtrainer",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@reatom/core": "^3.0.4",
        "@reatom/npm-react": "^3.0.4",
        "@testing-library/jest-dom": "^5.16.2",
        "@testing-library/react": "^12.1.4",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.4.1",
        "@types/node": "^16.11.26",
        "@types/react": "^17.0.40",
        "@types/react-dom": "^17.0.13",
        "airbnb": "^0.0.2",
        "classnames": "^2.3.1",
        "concurrently": "^7.1.0",
        "cross-env": "^7.0.3",
        "electron-is-dev": "^2.0.0",
        "node-sass": "^7.0.1",
        "prettier-eslint": "^14.0.0",
        "react": "^17.0.2",
        "react-contenteditable": "^3.3.6",
        "react-dom": "^17.0.2",
        "react-router": "^6.4.3",
        "react-router-dom": "^6.4.3",
        "react-scripts": "5.0.0",
        "typed-scss-modules": "7.0.1",
        "typescript-plugin-css-modules": "^3.4.0",
        "use-query-params": "^2.1.2",
        "wait-on": "^6.0.1",
        "web-vitals": "^2.1.4"
    },
    "main": "public/electron.js",
    "scripts": {
        "react-build": "react-scripts build --mode production",
        "react-test": "react-scripts test --env=jsdom",
        "react-eject": "react-scripts eject",
        "electron-build": "electron-builder",
        "release": "npm run react-build && electron-builder --publish=always",
        "build": "npm run react-build && npm run electron-build",
        "start": "react-scripts start --mode development",
        "start-all": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on $LOCAL_URL && electron .\""
    },
    "description": "Simple typing trainer",
    "author": "Fusted",
    "build": {
        "appId": "<com.typing>"
    },
    "homepage": "./",
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.20.0",
        "@typescript-eslint/parser": "^5.20.0",
        "electron": "^18.2.0",
        "eslint": "^8.14.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.29.4",
        "eslint-plugin-react-hooks": "^4.5.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "prettier": "2.6.2",
        "typescript": "^4.6.3"
    }
}
